                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <div class="row my-3">

                        <div class="col-4">
                            <div style="background-color: white;" class="p-3 text-gray-50 rounded shadow-sm">

                                <div class="text-center">
                                    <img class="rounded-circle shadow-sm" 
                                    style="background-color: #fff; border: 1px solid #dddfeb; padding: 0.25rem; border-radius: 0.35rem;" 
                                    src="/<%= account.avatar %>" alt="" width="150px" height="150px">
    
                                    <h5 class="text-uppercase my-2"><%= account.fullname %></h5>
                                </div>

                                <hr>

                                <p>
                                    <b><i class="bi bi-envelope"></i> Email: </b><%= account.email %>
                                </p>
                                <p>
                                    <b><i class="bi bi-phone"></i> Số điện thoại: </b><%= account.phone %>
                                </p>
                                <p>
                                    <% if (account.gender == 'Female') { %>
                                        <b><i class="bi bi-gender-ambiguous"></i> Giới tính: </b>Nữ
                                    <% } else { %>
                                        <b><i class="bi bi-gender-ambiguous"></i> Giới tính: </b>Nam
                                    <% } %>
                                </p>
                                <p>
                                    <b><i class="bi bi-calendar2-event"></i> Ngày sinh: </b><script>displayBirthday('<%= account.birthday %>')</script>
                                </p>
                                <p>
                                    <b><i class="bi bi-house"></i> Địa chỉ: </b><%= account.address %>
                                </p>

                                <hr>

                                <% var countOrderSuccess = 0, countOrderFail = 0, totalMoney = 0, countCommentRating = 0, totalRatingPoint = 0 %>

                                <% if (orderList != null) { %>
                                    <% orderList.forEach((order, index) => { %>
                                        <% if(order.status == 'Giao thành công') { %>
                                            <% countOrderSuccess += 1 %>
                                            <% totalMoney += order.totalPrice %>
                                        <% } %>
    
                                        <% if(order.status == 'Đã hủy') { %>
                                            <% countOrderFail += 1 %>
                                        <% } %>
                                    <% }) %>
                                <% } %>

                                <% if (commentRatingList != null) { %>
                                    <% commentRatingList.forEach((commentRating, index) => { %>
                                        <% countCommentRating += 1 %>
                                        <% totalRatingPoint += commentRating.ratingPoint %>
                                    <% }) %>
                                <% } %>

                                <p>
                                    <b><i class="bi bi-bag-check"></i> Đơn hàng đã mua: </b><%= countOrderSuccess %>
                                </p>
                                <p>
                                    <b><i class="bi bi-bag-x"></i> Đơn hàng đã hủy: </b><%= countOrderFail %>
                                </p>
                                <p>
                                    <b><i class="bi bi-cash"></i> Tiền đã trả: </b><script>displayNumber2VND('<%= totalMoney %>')</script>
                                </p>
                                <p>
                                    <b><i class="bi bi-chat-dots"></i> Số bình luận: </b><%= countCommentRating %> lần
                                </p>
                                <p>
                                    <b><i class="bi bi-star-half"></i> Điểm đánh giá: </b><%= (totalRatingPoint/countCommentRating).toPrecision(2) %>
                                </p>

                                <hr>

                                <div class="text-center">
                                    <a href="tel: <%= account.phone %>" class="btn btn-sm btn-circle btn-outline-primary"><i class="bi bi-telephone"></i></a>
                                    <button class="btn btn-sm btn-circle btn-outline-warning"><i class="bi bi-pencil-square"></i></button>
                                    <button class="btn btn-sm btn-circle btn-outline-danger"><i class="bi bi-trash"></i></button>
                                </div>

                            </div>
                        </div>

                        <div class="col-8">
                            <ul class="nav nav-pills nav-justified mb-3" id="pills-tab" role="tablist">
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link active" id="pills-customer-order-tab" data-toggle="pill" href="#pills-customer-order" role="tab" aria-controls="pills-home" aria-selected="true">
                                        <i class="bi bi-receipt-cutoff"></i> Đơn hàng
                                    </a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="pills-comment-rating-tab" data-toggle="pill" href="#pills-comment-rating" role="tab" aria-controls="pills-profile" aria-selected="false">
                                        <i class="bi bi-chat-dots"></i> Bình luận và đánh giá
                                    </a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#pills-contact" role="tab" aria-controls="pills-contact" aria-selected="false">
                                        <i class="bi bi-clock-history"></i> Lịch sử hoạt động
                                    </a>
                                </li>
                            </ul>
                            <div class="tab-content" id="pills-tabContent">
                                
                                <div class="tab-pane fade show active" id="pills-customer-order" role="tabpanel" aria-labelledby="pills-customer-order-tab">
                                    
                                    <div class="card shadow mb-4">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold text-primary">Danh sách đơn hàng của khách hàng</h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="table-responsive">
                                                <table class="table table-bordered" id="allCustomerOrderDataTable" width="100%" cellspacing="0">
                                                    <thead>
                                                        <tr>
                                                            <th>Mã đơn</th>
                                                            <th>Tổng tiền</th>
                                                            <th>Ngày đặt hàng</th>
                                                            <th>Trạng thái</th>
                                                        </tr>
                                                    </thead>
                                                    <tfoot>
                                                        <tr>
                                                            <th>Mã đơn</th>
                                                            <th>Tổng tiền</th>
                                                            <th>Ngày đặt hàng</th>
                                                            <th>Trạng thái</th>
                                                        </tr>
                                                    </tfoot>
                                                    <tbody>
                                                        <% if (orderList != null) { %>
                                                            <% orderList.forEach((order, index) => { %>
                                                                <tr>
                                                                    <td>
                                                                        <b>
                                                                            <a href="/admin/detail-order?idOrder=<%=order.idOrder%>&idAccount=<%=order.idAccount%>">#DH<%= order.idOrder %></a>
                                                                        </b>
                                                                    </td>
                                                                    <td><script>displayNumber2VND('<%= order.totalPrice %>')</script></td>
                                                                    <td><script>displayOrderDatetime('<%= order.orderDate %>')</script></td>
                                                                    <td>
                                                                        <% if(order.status == 'Chờ duyệt') { %>
                                                                            <span class="badge badge-pill badge-warning"><%= order.status %> <i class="bi bi-hourglass"></i></span>
                                                                        <% } %>
                                                                        <% if(order.status == 'Chuẩn bị') { %>
                                                                            <span class="badge badge-pill badge-info"><%= order.status %> <i class="bi bi-box-seam"></i></span>
                                                                        <% } %>
                                                                        <% if(order.status == 'Đang vận chuyển') { %>
                                                                            <span class="badge badge-pill badge-primary"><%= order.status %> <i class="bi bi-truck"></i></span>
                                                                        <% } %>
                                                                        <% if(order.status == 'Giao thành công') { %>
                                                                            <span class="badge badge-pill badge-success"><%= order.status %> <i class="bi bi-check2-circle"></i></span>
                                                                        <% } %>
                                                                        <% if(order.status == 'Đã hủy') { %>
                                                                            <span class="badge badge-pill badge-danger"><%= order.status %> <i class="bi bi-x-circle"></i></span>
                                                                        <% } %>
                                                                    </td>
                                                                </tr>
                                                            <% }) %>
                                                        <% } %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
        
                                </div>

                                <div class="tab-pane fade" id="pills-comment-rating" role="tabpanel" aria-labelledby="pills-comment-rating-tab">

                                    <div class="my-3 p-3 bg-white rounded shadow">

                                        <% if (commentRatingList == null) { %>

                                            
                                        <% } else { %>
                                            <% commentRatingList.forEach((commentRating, index) => {%>
                                                <div class="media text-muted pt-3">
                                                    <img class="mr-2 rounded-circle img-thumbnail" width="64px" height="64px" src="/<%= commentRating.avatar %>">
        
                                                <% if(index < commentRatingList.length - 1){ %>
                                                    <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                                <% } else { %>
                                                    <p class="media-body pb-3 mb-0 lh-125">
                                                <% } %>
                                                        <strong class="d-block text-gray-dark"><%= commentRating.fullname %> 
                                                            <span class="ml-2">
                                                                <script>displayRatingPoint2Star('<%= commentRating.ratingPoint %>')</script>
                                                            </span>
                
                                                            <span style="float: right;"><script>displayCommentDatetime('<%= commentRating.commentDate %>')</script></span>
                                                        </strong>
                
                                                        <span>
                                                            <%= commentRating.commentContent %>
                                                        </span>
    
                                                        <i><small>- đánh giá sản phẩm <a href="/admin/detail-product?idProduct=<%= commentRating.idProduct %>"><%= commentRating.name %></a></small></i>
                
                                                        <div class="dropdown no-arrow">
                                                            <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                                                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                                <i class="fas fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                                                            </a>
                                                            <div class="dropdown-menu dropdown-menu-right shadow animated--fade-in"
                                                                aria-labelledby="dropdownMenuLink">
                                                                <div class="dropdown-header">Thao tác</div>
                                                                <a class="dropdown-item" href="#">Xóa bình luận này</a>
                                                            </div>
                                                        </div>
                                                    </p>
    
    
                                                </div>
                                            <% }) %>
                                        <% } %>
    
                                    </div>

                                </div>

                                <div class="tab-pane fade" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab">
                                    
                                    <div class="card shadow">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold text-primary">Lịch sử hoạt động của khách hàng</h6>
                                        </div>
                                        <div class="card-body">
                                            <ul class="timeline">

                                                <% historyList.forEach((history, index) => {%>

                                                    <% if(history.type == 'account') { %>
                                                        <li class="timeline-item">
                                                            <strong>Tài khoản được tạo thành công</strong>
                                                            <span class="float-end text-muted text-sm"><script>displayHistoryDate('<%= history.date %>')</script></span>
                                                            <p>Khách hàng tạo tài khoản trực tiếp trên website của cửa hàng</p>
                                                        </li>
                                                    <% } %>

                                                    <% if(history.type == 'comment') { %>
                                                        <li class="timeline-item">
                                                            <strong>Bình luận và đánh giá sản phẩm</strong>
                                                            <span class="float-end text-muted text-sm"><script>displayHistoryDatetime('<%= history.date %>')</script></span>
                                                            <p>Khách hàng đã bình luận và đánh giá cho sản phẩm  <a href="/admin/detail-product?idProduct=<%= history.data.idProduct %>">#SP<%= history.data.idProduct %></a></p>
                                                        </li>
                                                    <% } %>
                                                <% }) %>
                                            </ul>
                                        </div>
                                    </div>

                                </div>
                        </div>

                    </div>

                   

                    
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; By Dao Thi Hien 2022</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="/logout">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="vendor/chart.js/Chart.min.js"></script>

    <!-- Page level plugins -->
    <script src="vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="vendor/datatables/dataTables.bootstrap4.min.js"></script>
    
    <!-- Page level custom scripts -->
    <script src="js/datatable.js"></script>
    
</body>

</html>