    <style>
        .timeline {
            list-style-type: none;
            position: relative;
        }

        .timeline-item:before, .timeline:before{
            content: " ";
            display: inline-block;
            position: absolute;
            z-index: 1;
        }

        .timeline-item:before {
            background: #fff;
            border: 3px solid #3f80ea;
            border-radius: 50%;
            height: 20px;
            left: 0;
            width: 20px;
        }

        .timeline:before {
            background: #dee6ed;
            height: 100%;
            left: 9px;
            width: 2px;
        }
    </style>

