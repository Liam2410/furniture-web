                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <div style="background-color: white;background-image:url(img/corner.png); 
                    background-position: right; background-repeat: no-repeat;opacity: 0.7; 
                    background-size: contain;" class="d-flex align-items-center p-3 my-3 text-dark-50 rounded shadow-sm">
                        <div class="lh-100">
                            <h3 style="color: black;" class="text-uppercase">Chi tiết đơn hàng: #DH<%= order.id %></h3>
                            <h6 class="mb-0 text-dark lh-100">
                                <script>displayOrderDatetime('<%= order.orderDate %>')</script>
                            </h6>
                            
                            <h5 class="text-dark mt-3">
                                Trang thái: 
                                <% if(order.status == 'Chờ duyệt') { %>
                                    <span class="badge badge-pill badge-warning"><%= order.status %> (ngày <script>displayChangeStatusDateTime('<%= order.statusChangeDate %>')</script>) <i class="bi bi-hourglass"></i></span>
                                <% } %>
                                <% if(order.status == 'Chuẩn bị') { %>
                                    <span class="badge badge-pill badge-info"><%= order.status %> (ngày <script>displayChangeStatusDateTime('<%= order.statusChangeDate %>')</script>) <i class="bi bi-box-seam"></i></span>
                                <% } %>
                                <% if(order.status == 'Đang vận chuyển') { %>
                                    <span class="badge badge-pill badge-primary"><%= order.status %> (ngày <script>displayChangeStatusDateTime('<%= order.statusChangeDate %>')</script>) <i class="bi bi-truck"></i></span>
                                <% } %>
                                <% if(order.status == 'Giao thành công') { %>
                                    <span class="badge badge-pill badge-success">
                                        <%= order.status %> (ngày <script>displayChangeStatusDateTime('<%= order.statusChangeDate %>')</script>) <i class="bi bi-check2-circle"></i>
                                    </span>
                                <% } %>
                                <% if(order.status == 'Đã hủy') { %>
                                    <span class="badge badge-pill badge-danger"><%= order.status %> (ngày <script>displayChangeStatusDateTime('<%= order.statusChangeDate %>')</script>) <i class="bi bi-x-circle"></i></span>
                                <% } %>

                            </h5>
                            
                            <div class="mt-3">

                                <span class="dropright">
                                    <button class="btn btn-sm btn-outline-dark dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        Đổi trạng thái
                                    </button>
                                    <div class="dropdown-menu animated--fade-in bg-light" aria-labelledby="dropdownMenuButton">
                                        <a class="dropdown-item" 
                                        href="/admin/change-order-status?idOrder=<%=order.idOrder%>&status=Chuẩn bị&idAccount=<%=account.id%>">Chuẩn bị</a>
                                        <a class="dropdown-item" 
                                        href="/admin/change-order-status?idOrder=<%=order.idOrder%>&status=Đang vận chuyển&idAccount=<%=account.id%>">Đang vận chuyển</a>
                                        <a class="dropdown-item" 
                                        href="/admin/change-order-status?idOrder=<%=order.idOrder%>&status=Giao thành công&idAccount=<%=account.id%>">Giao thành công</a>
                                        <div class="dropdown-divider"></div>
                                        <a class="dropdown-item" 
                                        href="/admin/change-order-status?idOrder=<%=order.idOrder%>&status=Đã hủy&idAccount=<%=account.id%>">Hủy đơn hàng</a>
                                    </div>
                                </span>
    
                                <span class="btn btn-sm btn-outline-primary" data-toggle="modal" data-target="#shippingFeeModal"><i class="bi bi-truck"></i></span>

                                <span class="btn btn-sm btn-outline-info" onclick="printBill()"><i class="bi bi-printer"></i></span>
    
                                <span class="btn btn-sm btn-outline-danger" data-toggle="modal" data-target="#confirmDeleteOrderModal"><i class="bi bi-trash"></i></span>

                                <script>
                                    function printBill() {
                                        var mywindow = window.open('', 'PRINT');

                                        mywindow.document.write('<html><head><link href="css/sb-admin-2.min.css" rel="stylesheet"><title>In hóa đơn</title>');
                                        mywindow.document.write('</head><body >');
                                        mywindow.document.write('<h1>HÓA ĐƠN</h1>');
                                        mywindow.document.write(document.getElementById('bill').innerHTML);
                                        mywindow.document.write('</body></html>');

                                        mywindow.document.close();
                                        mywindow.onload = function () {
                                            mywindow.focus();

                                            mywindow.print();
                                        };
                                    }
                                </script>

                            </div>

                        </div>
                    </div>

                    <!-- Shipping Fee Modal -->
                    <div class="modal fade" id="shippingFeeModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <form action="/admin/change-shipping-fee?idOrder=<%=order.idOrder%>&idAccount=<%=account.id%>" method="post">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="exampleModalLabel">Chỉnh sửa phí vận chuyển</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="form-group">
                                            <label for="shippingFeeInput">Phí vận chuyển</label>
                                            <input name="shippingFee" value="<%= order.shippingFee %>" type="number" class="form-control" id="shippingFeeInput" aria-describedby="emailHelp">
                                            <small id="emailHelp" class="form-text text-muted">Lưu ý phí vận chuyển sẽ không tính vào các loại doanh thu và thống kê.</small>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="submit" class="btn btn-primary">Cập nhật</button>
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                    <!-- Confirm Delete Order Modal -->
                    <div class="modal fade" id="confirmDeleteOrderModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <form action="/admin/change-shipping-fee?idOrder=<%=order.idOrder%>&idAccount=<%=account.id%>" method="post">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="exampleModalLabel">Xác nhận</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <p class="text-danger text-center display-1">
                                            <i class="bi bi-x-circle"></i>
                                        </p>
                                        <h5 class="text-center">Bạn có chắc chắn muốn xóa đơn hàng này ?</h5>
                                        <p class="text-center">Lưu ý bạn sẽ không thể khôi phục lại đơn hàng này</p>
                                    </div>
                                    <div class="modal-footer">
                                        <a href="/admin/delete-order?idOrder=<%=order.idOrder%>" class="btn btn-primary">Xóa</a>
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Không</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                    <div style="background-color: white;" class="d-flex align-items-start p-3 my-3 text-dark-50 rounded shadow-sm">

