                            <h6 class="mb-0 text-dark lh-100">
                                Phone: <%= account.phone %>
                            </h6>
                        </div>

                        <div class="lh-100 w-100">

                            <h5><b>Địa chỉ giao hàng</b></h5>

                            <h6 class="mb-0 text-dark lh-100"><%= order.shippingAddress %></h6>
                        </div>

                        <div class="lh-100 w-100">

                            <h5><b>Ghi chú của khách hàng</b></h5>

                            <h6 class="mb-0 text-dark lh-100"><%= order.note %></h6>
                        </div>
                    </div>

                    <div style="background-color: white;" class="d-flex align-items-start p-3 my-3 text-dark-50 rounded shadow-sm">
                        <div id="bill" class="table-responsive fs--1">
                            <table class="border-bottom table table-striped">
                                <thead class="bg-200 text-900">
                                    <tr>
                                        <th class="border-0">Sản phẩm</th>
                                        <th class="border-0 text-center">Số lượng</th>
                                        <th class="border-0 text-end">Đơn giá</th>
                                        <th class="border-0 text-end">Tổng số</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    <% productOrderList.forEach((product, index) => { %>
                                        <tr class="border-200">
                                            <td class="align-middle">
                                                <h6 class="mb-0 text-nowrap"><%= product.name %></h6>
                                                <% if (product.price > product.productPrice) { %>
                                                    <small class="mb-0">Áp dụng khuyến mãi <%= (product.price - product.productPrice)*100/product.price %>%</small>
                                                <% } %>
                                            </td>
                                            <td class="align-middle text-center"><%= product.quantityProduct %></td>
                                            <td class="align-middle text-end"><script>displayNumber2VND('<%= product.productPrice %>')</script></td>
                                            <td class="align-middle text-end"><script>displayNumber2VND('<%= product.productPrice * product.quantityProduct %>')</script></td>
                                        </tr>
                                    <% }) %>

                                </tbody>
                            </table>
                            <div class="g-0 justify-content-end row">
                                <div class="col-auto">
                                    <table class="fs--1 text-end table table-sm table-borderless">
                                        <tbody>
                                            <tr>
                                                <th class="text-900">Tổng tiền đơn hàng:</th>
                                                <td class="fw-semi-bold"><script>displayNumber2VND('<%= order.totalPrice %>')</script></td>
                                            </tr>
                                            <tr>
                                                <th class="text-900">
                                                    Phí vận chuyển:
                                                </th>
                                                <td class="fw-semi-bold"><script>displayNumber2VND('<%= order.shippingFee %>')</script></td>
                                            </tr>
                                            <tr class="border-top">
                                                <th class="text-900">Tổng phải trả:</th>
                                                <td class="fw-semi-bold"><script>displayNumber2VND('<%= order.totalPrice + order.shippingFee %>')</script></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; By Dao Thi Hien 2022</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="/logout">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>

</body>

</html>